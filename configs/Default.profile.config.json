  "Content": {
    "Metadata": {
      "Title": "Profile installation configuration file",
      "Filename": "Default.profile.config.json",
      "Tags": "powershell, configuration, installation, profile",
      "Author": "Janik von Rotz",
      "AuthorContact": "www.janikvonrotz.ch",
      "CreateDate": "2013-04-11",
      "LastEditDate": "2013-04-11",
      "Version": "1.0.0"
    },
    "Configuration": {
      "SystemVariables": {
        "SystemVariable": [
          {
            "Value": "\\functions",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          },
          {
            "Value": "\\apps\\Git\\cmd",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          },
          {
            "Value": "\\apps\\Git\\bin",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          },
          {
            "Value": "..\\SysInternals",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          },
          {
            "Value": "..\\Putty",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          },
          {
            "Value": "..\\Node.js",
            "Name": "Path",
            "RelativePath": "true",
            "Target": "Machine"
          }
        ]
      },
      "Features": {
        "Feature": [
          {
            "Name": "Git Update Task",
            "Install": "Disable"
          },
          {
            "Name": "Powershell Remoting",
            "Install": "Disable"
          }
        ]
      },
      "RegistryEntries": {
        "RegistryEntry": [
          {
            "Path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
            "Name": "HideFileExt",
            "Value": "0"
          },
          {
            "Path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Search\\PrimaryProperties\\UnindexedLocations",
            "Name": "SearchOnly",
            "Value": "00000000"
          }
        ]
      }
    }
  }
