	<Server Key="linux1" Name="ServerLinux1" User="Root" Description="Linux Proxy" PrivatKey="$PSProfilePath\ssh\Private.Key">
		<Protocol Name="scp" Port="72"></Protocol>	
	</Server>

	<Server Key="ServerSharePoint1" Name="ServerSharePoint1" User="Domain\Administrator" Description="SharePoint 2010 Server" SnapIns="Microsoft.SharePoint.PowerShell">
		<Protocol Name="rdp" Port="605"></Protocol>
	</Server>
	
	<Server Key="fw1" Name="FireWall1" User="Admin" Description="Firwall Router" PrivatKey="C:\Private.Key">
		<Protocol Name="ssh" Port="50"></Protocol>
		<Protocol Name="https" Port=""></Protocol>	
	</Server>
	
